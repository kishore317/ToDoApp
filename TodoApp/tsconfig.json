{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.js",
    "**/*.jsx",
    ".expo/types/**/*.ts",
    "app/(tabs)/_layout.jsx",
    ".expo-env.d.ts",
    "app/explore.jsx",
    "app/index.jsx"
  ]
}
